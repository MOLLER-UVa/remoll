<tube name="helicoil_outer_solid" rmin="helicoil_m[1,1]" rmax="helicoil_m[1,2]" z="helicoil_m[1,3]" deltaphi="360"  startphi="0" aunit="deg" lunit="mm"/>
<tube name="helicoil_center_solid" rmin="helicoil_m[2,1]" rmax="helicoil_m[2,2]" z="helicoil_m[2,3]" deltaphi="360"  startphi="0" aunit="deg" lunit="mm"/>
<tube name="helicoil_inner_solid" rmin="helicoil_m[3,1]" rmax="helicoil_m[3,2]" z="helicoil_m[3,3]" deltaphi="360"  startphi="0" aunit="deg" lunit="mm"/>

<!--create massive multiunion for hold all the helicoil -->
<multiUnion name="helicoilall_multiUnion" >
  <multiUnionNode name="node-0">
      <solid ref="helicoil_outer_solid" />
      <positionref ref="helicoil_outer_position" />
  </multiUnionNode>
  <multiUnionNode name="node-1">
      <solid ref="helicoil_center_solid" />
      <positionref ref="helicoil_center_position" />
  </multiUnionNode>
  <multiUnionNode name="node-2">
      <solid ref="helicoil_inner_solid" />
      <positionref ref="helicoil_inner_position" />
  </multiUnionNode>
</multiUnion>
